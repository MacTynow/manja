<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Prices" table="PRICES">
    <id name="idprices" type="integer" column="idPRICES">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="price" type="integer" column="price" nullable="false"/>
    <field name="dateBegin" type="date" column="date_begin" nullable="false"/>
    <field name="dateEnd" type="date" column="date_end" nullable="true"/>
    <field name="current" type="boolean" column="current" nullable="false"/>
  </entity>
</doctrine-mapping>
