{% extends "::layout.html.twig" %}
 
{% block body %}
    {% for key, message in app.session.flashbag.all() %}
        <div class="alert alert-{{ key }}">
            {{ message|trans({}, 'FOSUserBundle') }}
        </div>
    {% endfor %}

    <br/>

    <div class="row-fluid">
    <div class="span12">
        <div class="connection">
            {% block fos_user_content %}
            {% endblock fos_user_content %}
        </div>
    </div>
    </div>
{% endblock %}
